        <oboInOwl:id rdf:datatype="http://www.w3.org/2001/XMLSchema#string">VIRO:0000150</oboInOwl:id>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MARTX toxins</rdfs:label>
    </owl:Class>
